{
    "name": "blockies",
    "version": "0.0.0",
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev:server": "vite",
        "dev:css": "npx tailwindcss -o src/index.css --jit --watch --minify",
        "build": "tsc && vite build && echo '/* /index.html  200' | cat >build/_redirects",
        "serve": "vite preview"
    },
    "dependencies": {
        "react": "^17.0.0",
        "react-dom": "^17.0.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.8",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "npm-run-all": "^4.1.5",
        "react-router-dom": "^5.2.0",
        "tailwindcss": "^2.2.7",
        "typescript": "^4.3.2",
        "vite": "^2.4.4"
    }
}
